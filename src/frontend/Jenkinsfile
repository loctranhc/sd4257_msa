pipeline {
    agent any
    stages {
        stage('Build Image') {
            steps {
                dir('src'){
                    sh 'ls'
                }
                sh 'docker build -f /src/frontend -t fontend .'
            }
        }
        stage('Push Image to ACR') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}